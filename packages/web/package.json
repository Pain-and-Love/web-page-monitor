    "raw-loader": "^4.0.2",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "sass": "^1.49.0",
    "socket.io-client": "~4.4.1"
  },
  "devDependencies": {
    "@types/node": "~16.13.2",
    "@types/react": "~17.0.38",
    "concurrently": "~7.0.0",
