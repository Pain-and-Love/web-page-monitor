{
  "name": "@webest/web-page-monitor-web",
  "private": true,
  "license": "See monorepo's LICENSE.md",
  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"next dev -p 3002\" \"wait-on http://localhost:3002/ && start http://localhost:3002/\"",
    "build": "cross-env NEXT_distDir=.next2 next build && node copyNext.js",
    "static": "next build && next export -o monitor",
    "static-zh": "next build && next export -o monitor/zh",
    "static:en": "next build && cross-env-shell NEXT_PUBLIC_export_lang=en next export -o monitor_en",
    "static:zh": "next build && cross-env-shell NEXT_PUBLIC_export_lang=zh NEXT_PUBLIC_basePath=/zh next export -o monitor_zh",
    "start": "npm run build && pm2 start ecosystem.config.js --time && pm2 save",
    "nextstart": "next start -p 3002",
    "stop": "pm2 stop web --watch && pm2 delete web && pm2 save --force",
    "start-bak": "npm run static && concurrently \"next start -p 3002\" \"npx serve out -p 3001\"",
    "lint": "next lint"
  },
  "dependencies": {
    "@monaco-editor/react": "~4.3.1",
    "@types/node": "~16.11.25",
    "@types/react-table": "^7.7.9",
    "@types/styled-components": "^5.1.22",
    "@webest/web-page-monitor-esm-loader": "*",
    "@webest/web-page-monitor-helper": "*",
    "fs-extra": "^10.0.1",
    "i18next": "~21.6.11",
    "immer": "~9.0.12",
    "jose": "~4.5.0",
    "jotai": "~1.5.2",
    "js-cookie": "~3.0.1",
    "json-bigint": "^1.0.0",
    "jsrsasign": "^10.5.6",
    "mongodb": "=4.3.1",
    "next": "~12.0.8",
    "next-api-middleware": "~1.0.1",
    "optics-ts": "~2.3.0",
    "puppeteer-core": "10.0.0",
    "raw-loader": "~4.0.2",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-i18next": "~11.15.4",
    "react-table": "~7.7.0",
    "sass": "~1.49.0",
    "socket.io-client": "~4.4.1",
    "styled-components": "~5.3.3"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.1",
    "@types/json-bigint": "^1.0.1",
    "@types/react": "~17.0.38",
    "concurrently": "~7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "~8.6.0",
    "eslint-config-next": "~12.0.8",
    "monaco-editor": "~0.31.1",
    "typescript": "~4.5.4",
    "wait-on": "^6.0.0"
  }
}
